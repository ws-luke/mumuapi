                  "type": "string",
                  "example": "example@test.com"
                },
                "password": {
                  "type": "string",
                  "example": "example"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "402": {
            "description": "Payment Required"
          }
        }
      }
    },
